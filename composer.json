{
    "name": "monolyth/envy",
    "description": "Flexible environment handler for PHP projects (including unit tests!)",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Marijn Ophorst",
            "email": "marijn@monomelodies.nl"
        }
    ],
    "require": {
        "monomelodies/kingconf": "^1.1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.1.6"
    },
    "autoload": {
        "psr-4": {
            "Monolyth\\Envy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Monolyth\\Envy\\": "src/",
            "Monolyth\\Envy\\Tests\\": "tests/"
        }
    }
}
