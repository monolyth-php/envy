{
    "name": "monomelodies/envy",
    "description": "Flexible environment handler for PHP projects (including unit tests!)",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Marijn Ophorst",
            "email": "marijn@monomelodies.nl"
        }
    ],
    "require": {
        "monomelodies/kingconf": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Envy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Envy\\": "src/",
            "Envy\\Tests": "tests/"
        }
    }
}
